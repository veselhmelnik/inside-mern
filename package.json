    "mern",
    "react"
  ],
  "author": "Nikita Domitrak <domitrak.work@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.3",
    "axios": "^1.3.4",
    "bcryptjs": "^2.4.3",
    "config": "^3.3.9",
    "cross-env": "^7.0.3",
    "express": "^4.18.2",
    "express-validator": "^6.15.0",
    "jsonwebtoken": "^9.0.0",
    "materialize-css": "^1.0.0-rc.2",
    "mongoose": "^7.0.0",
    "react-icons": "^4.8.0",
    "react-redux": "^8.0.5",
    "react-select": "^5.7.0",
    "redux-thunk": "^2.4.2",
    "shortid": "^2.2.16",
    "sweetalert2": "^11.7.3"
  },
  "devDependencies": {
    "concurrently": "^7.6.0",
    "nodemon": "^2.0.20",
